





local SolarisLib = loadstring(game:HttpGet("https://raw.githubusercontent.com/Stebulous/solaris-ui-lib/main/source.lua"))()

local playerlist = {}

for i,v in pairs(game.Players:GetPlayers())do
    if v ~= game.Players.LocalPlayer then
        table.insert(playerlist,v.Name)
    end
end


local win = SolarisLib:New({
  Name = "Dwarves VS Zombies",
  FolderToSave = "DwarvsZomb"
})

local tab1 = win:Tab("Misc")
local tab2 = win:Tab("Autofarm")
local tab3 = win:Tab("Disguise")
local tab4 = win:Tab("Zombie")
local tab5 = win:Tab("Dwarves")
local tab6 = win:Tab("Bandit")
local tab7 = win:Tab("Teleport")
local tab8 = win:Tab("Broken")
local tab9 = win:Tab("Settings")
local tab10= win:Tab("Tab 1")
local tab = win:Tab("Tab 1")
local tab = win:Tab("Tab 1")

local sec = tab1:Section("Misc")
local sec2 = tab2:Section("Autofarm")
local sec3 = tab3:Section("Disguise")
