------------------------------tab Misc----------------------------

local toggletext = sec:Toggle("2022 Texture", false,"Toggle", function(t)
    game:WaitForChild("MaterialService")["Use2022Materials"] = t
end)

local togglebur = sec:Toggle("Burn", false,"Toggle", function(t)
    game:GetService("ReplicatedStorage").Events.BurnZombie:FireServer(t) 
end)

local toggledep = sec:Toggle("Bank Gold [require Bank]", false,"Toggle", function(t)
_G.dep = t
while _G.dep do
for i,v in pairs(workspace:GetDescendants()) do
    if v.name == "Bank" then
        fireclickdetector(v.DepositButton.ClickDetector)
    end
end
task.wait()
end
end)

local togglenoti = sec:Toggle("No Notifications", false,"Toggle", function(t)
    _G.reme = t
    while _G.reme do
    for i,v in pairs(game.Players.LocalPlayer.PlayerGui:GetDescendants()) do
        if v.name == "GoldEarnedLabel" then
            v:Destroy()
        end
    end
    task.wait()
    end
end)


sec:Button("No Role Tag", function()
    for i,v in pairs(game.Players.LocalPlayer.Character.Head.role:GetDescendants()) do
        v:Destroy()
    end
end)



sec:Button("No Name Tag", function()
    for i,v in pairs(game.Players.LocalPlayer.Character.Head.playerName:GetDescendants()) do
        v:Destroy()
    end
end)


