---------------------------tab 4 sec 4 Zombie---------------
sec4:Textbox("Textbox", true, function(t)
    for _,patea in pairs(game.Players:GetChildren()) do
        if string.find(patea[searchmetd], t) then
            print(patea)
            playerselect = t
        end
    end
end)


local playerdropzom = sec4:Dropdown("Player", playerlist,"","Dropdown", function(t)
    print(t)
    playerselect = t
end)
sec4:Button("Refresh Dropdown", function()
    playerdropzom:Refresh(playerlist, true)
    SolarisLib:Notification("Refresh Dropdown", "successfully Refreshed the Dropdown.")
end)
sec4:Button("Be a Zombie without loosing Gold", function()
    for i,v in pairs(workspace:GetDescendants()) do
        if v.name == "1" then
            firetouchinterest(v, game.Players.LocalPlayer.Character.HumanoidRootPart, 0)
            wait()
            firetouchinterest(v, game.Players.LocalPlayer.Character.HumanoidRootPart, 1)
        end
    end
end)

sec4:Button("Damage Player [Require Staff]", function()
    local cord = game.Players.LocalPlayer.Character.Staff.FirePoint.CFrame
    game.Players.LocalPlayer.Character.Staff.RemoteEvent:FireServer(cord, workspace[playerselect].HumanoidRootPart)
end)

sec4:Button("Damage Player [Require Crossbow]", function()
    local cord = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
    game.Players.LocalPlayer.Character.Crossbow.RemoteEvent:FireServer(cord, workspace[playerselect].HumanoidRootPart)
end)


local toggledPalace = sec4:Toggle("Damage Palace", false,"Toggle", function(t)
    _G.palacedesto = t
    while _G.palacedesto do
        game.Players.LocalPlayer.Character.Punch.Strike:FireServer(workspace.Palace.StructureHealthPart)
        task.wait()
    end

end)

local toggledPalace = sec4:Toggle("Damage Gate", false,"Toggle", function(t)
    _G.palacedesto = t
    while _G.palacedesto do
        game.Players.LocalPlayer.Character.Punch.Strike:FireServer(game:GetService("Workspace").Walls.GateSection.StructureHealthPart)
        task.wait()
    end

end)
---------------------------tab5 sec 5 dwarf-------------
local playerdropdwarf = sec5:Dropdown("Player", playerlist,"","Dropdown", function(t)
    print(t)
    playerselectdr = t
end)
sec5:Button("Refresh Dropdown", function()
    playerdropdwarf:Refresh(playerlist, true)
    SolarisLib:Notification("Refresh Dropdown", "successfully Refreshed the Dropdown.")
end)
sec5:Button("Damage Player [Require Crossbow]", function()
    local cord = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
    game.Players.LocalPlayer.Character.Crossbow.RemoteEvent:FireServer(cord, workspace[playerselectdr].HumanoidRootPart)
end)

sec5:Button("Damage Player [Require Musket]", function()
    local cord = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
    game.Players.LocalPlayer.Character.Musket.RemoteEvent:FireServer(cord, workspace[playerselectdr].HumanoidRootPart)
end)

local toggle = sec5:Toggle("Repair Palace", false,"Toggle", function(t)
    _G.repairpalace = t 
    while _G.repairpalace do
        game.Players.LocalPlayer.Character.Hammer.RemoteEvent:FireServer(workspace.Palace.StructureHealthPart)
        task.wait()
    end
end)


----------------------------tab7 sec6 Teleport-------------
sec6:Button("Testing Place ", function()
    game:GetService("TeleportService"):Teleport(7171410657, LocalPlayer)
    SolarisLib:Notification("Teleporting", "You Are Bieng Teleported Plesae Wait")

end)

sec6:Button("Main Place", function()
    game:GetService("TeleportService"):Teleport(6834667991, LocalPlayer)
    SolarisLib:Notification("Teleporting", "You Are Bieng Teleported Plesae Wait")

end)

-----------------------tab8 sec 7 Broken-------------------------
local dropdown = sec7:Dropdown("Material", {"Stone","Wood","Food"},"","Dropdown", function(t)
    mater = t
end)


sec7:Button("Town Market", function()
    SolarisLib:Notification(""..mater, "The Town "..mater.." Market Has Been Bugged People sell The "..mater.." will get inf "..mater.." and inf Gold To Reset the Market to Normal The Server Must be Reset by Destroying the Palace")
    game:GetService("ReplicatedStorage").Events["Sell"..mater]:FireServer((0/0)/math.huge)
    
end)

sec7:Button("Bandit Market", function()
    SolarisLib:Notification(""..mater, "The Bandit "..mater.." Market Has Been Bugged People sell The "..mater.." will get inf "..mater.." and inf Gold This Action Cannot Be Undo")
    game:GetService("ReplicatedStorage").Events["Sell"..mater.."3"]:FireServer((0/0)/math.huge)
end)


------------------------tab9 sec8 settings--------
local dropdown = sec8:Dropdown("Player Search Method", {"Name","DisplayName"},"Name","Dropdown", function(t)
    searchmetd = t
end)

